import java.util.Scanner;

public class ArmstrongOrNot {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a number");
		int num=sc.nextInt();
		int sum=0;
		while(num>0)
		{
			int r=num%10;
			int cube=r*r*r;
			sum=sum+cube;
			num=num/10;
		}

	}
}
